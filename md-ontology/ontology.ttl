    dcterms:identifier "controversialFact_montanelli_2"^^xsd:string .

<https://github.com/KRKE-monument-debate-ontology/Data_MDO/md-ontology/controversialFact/controversialFact_savile_1> a mdo:ControversialFact ;
    rdfs:label "After his death, Jimmy Savile was accused of widespread sexual abuse of minors and adults, with investigations concluding that he had exploited his celebrity status to commit offences over several decades."^^xsd:string ;
